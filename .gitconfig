[user]
	name = Rachel Ober
	email = rachelober@gmail.com
[core]
	excludesfile = ~/.gitignore
	editor = vim
[alias]
	ci = commit -a -v
	co = checkout
	st = status -a
	praise = blame
	br = branch
	df = diff
	lg = log -p
	rb = rebase
[apply]
	whitespace = strip
[color]
	ui = true
  interactive = true
[color "branch"]
  current = yellow reverse
  local = yellow
  untracked = cyan
[color "diff"]
  meta = blue
  frag = black
  old = red
  new = green
  renames = copy
[color "status"]
  added = green
  updated = yellow
  changed = yellow reverse
  untracked = cyan
  nobranch = blue
[github]
	user = rachelober
[help]
  autocorrect = 1
[merge]
	tool = opendiff
[pager]
	color = true
