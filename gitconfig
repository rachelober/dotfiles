[user]
  name = Rachel Ober
  email = rachelober@users.noreply.github.com
[core]
  excludesfile = ~/.gitignore
  editor = vim
[alias]
  ci = commit -a -v
  co = checkout
  st = status -a
  praise = blame
  br = branch
  df = diff
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  rb = rebase
  pf = push --force-with-lease
  list-merged = !"git branch --merged | grep -v '\\*' | grep -v master | xargs -n 1"
  delete-merged = !"git branch --merged | grep -v '\\*' | grep -v master | xargs -n 1 git branch -d"
[apply]
  whitespace = strip
[color]
  ui = true
  interactive = true
[color "branch"]
  current = yellow reverse
  local = green
  remote = red
  plain = white
  untracked = cyan
[color "diff"]
  meta = blue
  frag = black
  old = red
  new = green
  renames = copy
  whitespace = blue reverse
[color "status"]
  added = yellow
  updated = green
  changed = red
  untracked = cyan
  nobranch = blue
[commit]
  template = ~/.gitmessage
[github]
  user = rachelober
[help]
  autocorrect = 1
[merge]
  tool = opendiff
[pager]
  color = true
[push]
  default = simple
